{
  "extends": ["plugin:@typescript-eslint/recommended", "plugin:react/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "node": false,
    "browser": true,
    "es2021": true
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
     "@typescript-eslint/no-unused-vars": 0
  }
}
